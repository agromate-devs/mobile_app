{
  "name": "agromate",
  "private": true,
  "version": "1.0.0",
  "description": "AgroMate",
  "repository": "",
  "license": "UNLICENSED",
  "scripts": {
    "start": "npm run dev",
    "dev": "cross-env NODE_ENV=development vite",
    "build": "cross-env NODE_ENV=production vite build",
    "build-capacitor-ios": "cross-env NODE_ENV=production vite build && npx cap copy ios",
    "build-capacitor-android": "cross-env NODE_ENV=production vite build && npx cap copy android",
    "copysqlwasm": "copyfiles -u 3 node_modules/sql.js/dist/sql-wasm.wasm public/assets",
    "postinstall": "cpy --flat ./node_modules/framework7-icons/fonts/*.* ./src/fonts/ && cpy --flat ./node_modules/material-icons/iconfont/*.* ./src/fonts/ && npm run copysqlwasm"
  },
  "browserslist": [
    "IOS >= 15",
    "Safari >= 15",
    "last 5 Chrome versions",
    "last 5 Firefox versions"
  ],
  "dependencies": {
    "@capacitor-community/sqlite": "^5.0.2",
    "@capacitor-firebase/app": "^5.0.0",
    "@capacitor-firebase/authentication": "^5.0.0",
    "@capacitor/android": "^5.0.3",
    "@capacitor/app": "^5.0.2",
    "@capacitor/browser": "^5.0.2",
    "@capacitor/core": "^5.0.3",
    "@capacitor/ios": "^5.0.3",
    "@capacitor/keyboard": "^5.0.2",
    "@capacitor/splash-screen": "^5.0.2",
    "@capacitor/status-bar": "^5.0.2",
    "@tsconfig/svelte": "^4.0.1",
    "copyfiles": "^2.4.1",
    "dom7": "^4.0.6",
    "firebase": "^9.22.0",
    "framework7": "^8.0.5",
    "framework7-icons": "^5.0.5",
    "framework7-svelte": "^8.0.5",
    "jeep-sqlite": "^2.3.6",
    "material-icons": "^1.13.6",
    "skeleton-elements": "^4.0.1",
    "svelte": "^3.59.1",
    "svelte-preprocess": "^5.0.4",
    "swiper": "^9.3.2",
    "typeorm": "^0.3.16",
    "typescript": "^5.1.3",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@capacitor/cli": "^5.0.3",
    "@sveltejs/vite-plugin-svelte": "^2.2.0",
    "cordova-res": "^0.15.4",
    "cpy-cli": "^4.2.0",
    "cross-env": "^7.0.3",
    "postcss-preset-env": "^8.3.2",
    "vite": "^4.3.7"
  }
}
